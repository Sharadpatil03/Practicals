
#include <bits/stdc++.h>
using namespace std;

// 0 1 1 2 3 5
int fibIterative(int n){
    if(n<=1){
        return n;
    }
    
    int prev2=0;  //f(0)
    int prev1=1;  //f(1)
    int current;
    for(int i=2; i<=n; i++){
        current=prev2+ prev1;
        prev2=prev1;
        prev1=current;
    }
    return current;
}

int fibRec(int n){
    if(n<=1){
        return n;
    }
    return fibRec(n-1)+ fibRec(n-2);
}

int main() {
int n;
cin>>n;
cout<<fibIterative(n)<<endl;

cout<<fibRec( n);

    return 0;
}
